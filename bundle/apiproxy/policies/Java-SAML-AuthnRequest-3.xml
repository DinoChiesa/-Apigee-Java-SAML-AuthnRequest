<JavaCallout name='Java-SAML-AuthnRequest-3'>
  <Properties>
    <Property name='output-variable'>authnrequest</Property>
    <Property name='private-key'>{my_private_key}</Property>
    <Property name='certificate'>{my_certificate}</Property>
    <Property name='issuer'>http://sp.example.com/demo1/metadata.php</Property>
    <Property name='acs-url'>https://sp.example.com/demo1/index.php?acs</Property>
    <Property name='destination'>https://idp.example.com/SSOService.php</Property>
    <Property name='service-provider-name'>TestServiceProvider</Property>
    <Property name='force-authn'>true</Property>
    <Property name='digest-method'>sha256</Property>
    <Property name='signature-method'>rsa-sha256</Property>
    <Property name='requester-id'>https://whatever.example.com/saml/metadata</Property>
    <Property name='name-id-format'>unspecified</Property>
  </Properties>
  <ClassName>com.google.apigee.edgecallouts.samlauthn.Generate</ClassName>
  <ResourceURL>java://apigee-samlauthn-20200713.jar</ResourceURL>
</JavaCallout>
